{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@fitmonitor/client-services": [
        "libs/shared/services/client-services/src/index.ts"
      ],
      "@fitmonitor/consts": ["libs/shared/consts/src/index.ts"],
      "@fitmonitor/data-access": [
        "libs/auth/client-auth/data-access/src/index.ts"
      ],
      "@fitmonitor/generators": ["tools/generators/src/index.ts"],
      "@fitmonitor/interfaces": ["libs/shared/interfaces/src/index.ts"],
      "@fitmonitor/schemas": ["libs/shared/schemas/src/index.ts"],
      "@fitmonitor/server-auth": ["libs/auth/server-auth/src/index.ts"],
      "@fitmonitor/server-calendar": [
        "libs/calendar/server-calendar/src/index.ts"
      ],
      "@fitmonitor/server-decorators": [
        "libs/shared/decorators/server-decorators/src/index.ts"
      ],
      "@fitmonitor/server-guards": [
        "libs/shared/guards/server-guards/src/index.ts"
      ],
      "@fitmonitor/server-interceptors": [
        "libs/shared/interceptors/server-interceptors/src/index.ts"
      ],
      "@fitmonitor/server-metrics": [
        "libs/metrics/server-metrics/src/index.ts"
      ],
      "@fitmonitor/server-schedule": [
        "libs/schedule/server-schedule/src/index.ts"
      ],
      "@fitmonitor/server-services": [
        "libs/shared/services/server-services/src/index.ts"
      ],
      "@fitmonitor/shell": [
        "libs/auth/client-auth/features/shell/src/index.ts"
      ],
      "@fitmonitor/shell/data-access": ["libs/shell/data-access/src/index.ts"],
      "@fitmonitor/shell/feature/shell": [
        "libs/shell/features/shell/src/index.ts"
      ],
      "@fitmonitor/sign-in": [
        "libs/auth/client-auth/features/sign-in/src/index.ts"
      ],
      "@fitmonitor/util": ["libs/shared/util/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
